<tr style="border-top: 2px solid #888;">
  <td rowspan="3" style="width: 20%;">
    <%= fund.abbr %>
    <br>
    <span style="font-size: 11px; font-weight: none;"><%= fund.name %></span>
  </td>
  <td>NAV</td>
  <% (NUMBER_OF_DAYS_TO_SHOW - 1).downto(0) do |i| %>
    <td><%= nav fund.nav_at @dates[i] %></td>
  <% end %>
</tr>
<tr>
  <td>diff</td>
  <% (NUMBER_OF_DAYS_TO_SHOW - 1).downto(0) do |i| %>
    <td style="color: <%= text_color fund.nav_at(@dates[i]), fund.nav_at(@dates[i + 1]) %>;"><%= diff fund.nav_at(@dates[i]), fund.nav_at(@dates[i + 1]) %></td>
  <% end %>
</tr>
<tr>
  <td>%</td>
  <% (NUMBER_OF_DAYS_TO_SHOW - 1).downto(0) do |i| %>
    <td style="color: <%= text_color fund.nav_at(@dates[i]), fund.nav_at(@dates[i + 1]) %>;"><%= percentage fund.nav_at(@dates[i]), fund.nav_at(@dates[i + 1]) %></td>
  <% end %>
</tr>
